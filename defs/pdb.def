SIZEHEADER	INT32
VERSION		LIST(INT)
NAME		STR
NBUSERS		INT
NBGROUPS	INT
NBTABLES	INT
SIZEUSERS	INT
SIZEGROUPS	INT
SIZETABLES	LIST(INT)
{w/ size SIZEUSERS
USERS:		(STR + HASH + LIST(INT)) * NBUSERS
 * NAME		STR
 * PASSWD	HASH[SHA258:32BYTES]
 * GROUPS	LIST(INT)
 }
{w/ size SIZEGROUPS
GROUPS: (STR + 8BYTES LIST(INT + 64BYTES)) * NBGROUPS
 * NAME		STR
 * SALT		8BYTES
 * RIGHTS:	LIST(INT + 64BYTES)
    * ID	INT
    * SAVESTATE	HASH[SHA512:64BYTES]
 }
TABLES: {(INT32 + STRING + 8BYTES + 32BYTES + LIST(INT + 64BYTES) + {e(INT + N*INT + INT) + SIZE*(?TYPES)})} * NBTABLES
 {w/ size SIZETABLE in SIZETABLES
 * SIZEHEADER	INT32
 * NAME		STRING
 * SALT		8BYTES
 * PASSWD	HASH[SHA256:32BYTES]
 * RIGHTS:	LIST(INT + 64BYTES)
    * ID	INT
    * SAVESTATE	HASH[SHA512:64BYTES]
 {encrypted untill the end w/ size SIZETABLE - (4[INT32] + SIZEHEADER)
 * HEADER: (LIST(INT) + LIST(STR) + INT)
    * TYPES	LIST(INT)
    * NAMES	LIST(STR)
    * SIZE	INT
 * LINES	SIZE * (?TYPES)
 }
 }
